{
  "name": "prettier-plugin-rdn",
  "version": "0.1.0",
  "description": "Prettier plugin for RDN (Rich Data Notation)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "prettierrc-schema.json", "README.md", "LICENSE"],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "prettier",
    "plugin",
    "rdn",
    "formatter"
  ],
  "license": "MIT",
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
