{
  "name": "@rdn/typescript",
  "version": "0.1.0",
  "description": "RDN (Rich Data Notation) reference implementation in TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "bench": "vitest bench",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "rdn",
    "json",
    "parser",
    "serializer"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
